<% include chunks/header %>
<script></script>
<div class="mainbody">
    <div id="main_video_frame">
        <h1><%= video.title %></h1> 
        <iframe id="primary_video" src="<%= video.url %>" frameborder="0" allowfullscreen></iframe>
        <div id="category_bar">
            <ul>
                <% categories.forEach(function(category) { %>
                    <li> <a href="<%='/watch/'+category%>"><%= category %></a></li>
                <% });%>                
            </ul>
        </div>
    </div>
</div>

